        <source>Set end time</source>
        <target state="translated">Establezca la hora de finalización</target>
      </trans-unit>
      <trans-unit id="select_calendars">
        <source>Select calendars</source>
        <target state="translated">Elegir calendarios</target>
      </trans-unit>
      <trans-unit id="save">
        <source>Save</source>
        <target state="translated">Grabar</target>
      </trans-unit>
