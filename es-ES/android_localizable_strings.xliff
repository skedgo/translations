        <target state="translated">No se pudo resolver la ubicación</target>
      </trans-unit>
      <trans-unit id="failed_2_resolve_link">
        <source>Failed to resolve link</source>
        <target state="translated">No se pudo resolver el enlace</target>
      </trans-unit>
      <trans-unit id="get_off">
        <source>Get Off</source>
        <target state="translated">Bajar</target>
      </trans-unit>
      <trans-unit id="first">
        <source>First:</source>
        <target state="translated">Primero:</target>
      </trans-unit>
      <trans-unit id="filter_timetable_hint">
        <source>Filter by service name, number or stop</source>
        <target state="translated">Filtrar por nombre de servicio, número o
          parada</target>
        <alt-trans>
          <target>Filtrar por nombre de servicio, número o parada</target>
        </alt-trans>
      </trans-unit>
