        <target state="translated">Agregar una ubicación</target>
      </trans-unit>
      <trans-unit id="add_to_contacts">
        <source>Add to contacts</source>
        <target state="translated">Agregar a contactos</target>
      </trans-unit>
      <trans-unit id="add_to_home_screen">
        <source>Add to Home screen</source>
        <target state="translated">Agregar en la pantalla de inicio</target>
      </trans-unit>
      <trans-unit id="agenda_tip">
        <source>View your daily agenda and favourites in the tabs below</source>
        <target state="translated">Ver su agenda diaria y los favoritos en las
          pestañas de abajo</target>
        <alt-trans>
          <target>Ver su agenda diaria y los favoritos en las pestañas de abajo</target>
        </alt-trans>
      </trans-unit>
