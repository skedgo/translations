        <source>There was an error and it was reported to us. Please try again</source>
        <target state="translated">Hubo un error y ya fue reportado. Por favor,
          intente nuevamente</target>
        <alt-trans>
          <target>Hubo un error y ya fue reportado. Por favor, intente nuevamente</target>
        </alt-trans>
      </trans-unit>
      <trans-unit id="origin">
        <source>origin</source>
        <target state="translated">origen</target>
      </trans-unit>
      <trans-unit id="outside_region_error">
        <source>Sorry, that location is outside of TripGo's range!</source>
        <target state="translated">Lo siento, esa ubicación está fuera del aclance
          de TripGo!</target>
        <alt-trans>
          <target>Lo siento, esa ubicación está fuera del aclance de TripGo!</target>
        </alt-trans>
      </trans-unit>
