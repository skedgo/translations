        <target state="translated">Definir hora</target>
      </trans-unit>
      <trans-unit id="set_start_time">
        <source>Set start time</source>
        <target state="translated">Definir hora de inicio</target>
      </trans-unit>
      <trans-unit id="set_location">
        <source>Set Location</source>
        <target state="translated">Definir ubicación</target>
      </trans-unit>
