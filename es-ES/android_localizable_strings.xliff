        <source>Information on concession pricing is subject to availability.</source>
        <target state="translated">La información relacionada a precios bonificados
          está sujeta a disponibilidad.</target>
        <alt-trans>
          <target>La información relacionada a precios bonificados está sujeta a disponibilidad.</target>
        </alt-trans>
      </trans-unit>
      <trans-unit id="its_time_to_leave">
        <source>It's time to leave!</source>
        <target state="translated">Es hora de salir!</target>
      </trans-unit>
      <trans-unit id="leave_now_to_make_on_time">
        <source>Leave now to make it on time</source>
        <target state="translated">Debe salir ahora para llegar a tiempo</target>
      </trans-unit>
      <trans-unit id="go_to_now">
        <source>Go to Now</source>
        <target state="translated">Ir ahora</target>
      </trans-unit>
      <trans-unit id="include_cal_explanation">
        <source>Events from those calendars will be used in planning your day</source>
        <target state="translated">Los eventos de esos calendarios serán usados
          para planificar tu día</target>
        <alt-trans>
          <target>Los eventos de esos calendarios serán usados para planificar tu día</target>
        </alt-trans>
      </trans-unit>
      <trans-unit id="minute">
        <source>1 minute</source>
        <target state="translated">1 minuto</target>
      </trans-unit>
      <trans-unit id="new_vehicle">
        <source>New vehicle</source>
        <target state="translated">Nuevo vehículo</target>
      </trans-unit>
      <trans-unit id="location_not_found">
        <source>Location Not Found</source>
        <target state="translated">Ubicación no encontrada</target>
