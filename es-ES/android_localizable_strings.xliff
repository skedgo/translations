        <target state="translated">No puede agregar recordatorios a viajes en el
          pasado</target>
        <alt-trans>
          <target>No puede agregar recordatorios a viajes en el pasado</target>
        </alt-trans>
      </trans-unit>
      <trans-unit id="cant_call_error">
        <source>Error placing call</source>
        <target state="translated">Error al realizar la llamada</target>
      </trans-unit>
      <trans-unit id="coordinates_s">
        <source>Coordinates: %s</source>
        <target state="translated">Coordenadas: %s</target>
      </trans-unit>
