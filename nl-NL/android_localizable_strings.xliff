        <target xml:lang="nl" state="translated">U bent hier</target>
      </trans-unit>
      <trans-unit id="news">
        <source>News</source>
        <target xml:lang="nl" state="translated">Nieuws</target>
        <note>Settings option to enable/disable news (notifications).</note>
      </trans-unit>
      <trans-unit id="include_in_trips">
