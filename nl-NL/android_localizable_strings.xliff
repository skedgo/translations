        <source>Events from those calendars will be used in planning your day</source>
        <target xml:lang="nl" state="translated">Activiteiten in deze kalenders zijn zichtbaar in uw TripGo agenda</target>
      </trans-unit>
      <trans-unit id="minute">
        <source>1 minute</source>
        <target xml:lang="nl" state="translated">1 minuut</target>
