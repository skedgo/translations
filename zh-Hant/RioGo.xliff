<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="urn:oasis:names:tc:xliff:document:1.2" version="1.2" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.2 http://docs.oasis-open.org/xliff/v1.2/os/xliff-core-1.2-strict.xsd">
  <file datatype="plaintext" original="RioGo/Localizable.strings" source-language="en" target-language="zh-Hant">
    <header>
      <tool build-num="7D175" tool-id="com.apple.dt.xcode" tool-name="Xcode" tool-version="7.3"/>
    </header>
    <body>
      <trans-unit id="%@ reviews">
        <source>%@ reviews</source>
        <note>as in 'number of' reviews, e.g., 100 reviews</note>
        <target xml:lang="zh-Hant" state="translated">%@ 個評價</target>
      </trans-unit>
      <trans-unit id="%@ tips">
        <source>%@ tips</source>
        <note>as in 'number of' tips, e.g., 100 tips</note>
        <target xml:lang="zh-Hant" state="translated">%@ 個小貼士</target>
      </trans-unit>
      <trans-unit id="About">
        <source>About</source>
        <note>Title for view that shows the 'About' info for an event</note>
        <target xml:lang="zh-Hant" state="translated">關於</target>
      </trans-unit>
      <trans-unit id="Add">
        <source>Add</source>
        <note>A title on the button that allows users to select the event for attendance.</note>
        <target xml:lang="zh-Hant" state="translated">加入</target>
        <alt-trans>
          <target xml:lang="zh-Hant">添加</target>
        </alt-trans>
      </trans-unit>
      <trans-unit id="Add another visit">
        <source>Add another visit</source>
        <note>A title on the button that, when tapped, allows users to add another visit for the selected event or attraction. Translation should not be too long!</note>
        <target xml:lang="zh-Hant" state="translated">新增到訪時間</target>
      </trans-unit>
      <trans-unit id="Add to agenda">
        <source>Add to agenda</source>
        <note>Title for button in detail view that, when tapped, adds the event to agenda</note>
        <target xml:lang="zh-Hant" state="needs-translation">Add to agenda</target>
      </trans-unit>
      <trans-unit id="Added">
        <source>Added</source>
        <note>A title on the button showing an event has been selected for attendance.</note>
        <target xml:lang="zh-Hant" state="translated">已添加</target>
      </trans-unit>
      <trans-unit id="Address">
        <source>Address</source>
        <note>Title for cell in detail view that shows the event's address</note>
        <target xml:lang="zh-Hant" state="translated">聯繫地址</target>
      </trans-unit>
      <trans-unit id="Afternoon">
        <source>Afternoon</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">下午</target>
      </trans-unit>
      <trans-unit id="Attraction">
        <source>Attraction</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">景點</target>
      </trans-unit>
      <trans-unit id="Attractions">
        <source>Attractions</source>
        <note>Search scope (category) name</note>
        <target xml:lang="zh-Hant" state="translated">景點</target>
      </trans-unit>
      <trans-unit id="Browse events and places">
        <source>Browse events and places</source>
        <note>Title for button that allows users to pick events &amp; places</note>
        <target xml:lang="zh-Hant" state="translated">流覽事件和景點</target>
      </trans-unit>
      <trans-unit id="Call">
        <source>Call</source>
        <note>Title for cell in detail view that shows the event's contact phone number</note>
        <target xml:lang="zh-Hant" state="translated">聯繫電話</target>
      </trans-unit>
      <trans-unit id="Cancel">
        <source>Cancel</source>
        <note>Title for Cancel button</note>
        <target xml:lang="zh-Hant" state="translated">取消</target>
      </trans-unit>
      <trans-unit id="Check in">
        <source>Check in</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">登記</target>
        <alt-trans>
          <target xml:lang="zh-Hant">入房</target>
        </alt-trans>
      </trans-unit>
      <trans-unit id="Check out">
        <source>Check out</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">退房</target>
      </trans-unit>
      <trans-unit id="Closed">
        <source>Closed</source>
        <note>Indicating a place has closed</note>
        <target xml:lang="zh-Hant" state="translated">不營業</target>
      </trans-unit>
      <trans-unit id="Date">
        <source>Date</source>
        <note>Title for a date (on which to visit an attraction)</note>
        <target xml:lang="zh-Hant" state="translated">日期</target>
      </trans-unit>
      <trans-unit id="Discover Rio and the Games">
        <source>Discover Rio and the Games</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">探索里約熱內盧與奧運</target>
      </trans-unit>
      <trans-unit id="Done">
        <source>Done</source>
        <note>Title for done button</note>
        <target xml:lang="zh-Hant" state="translated">完成</target>
      </trans-unit>
      <trans-unit id="Download">
        <source>Download</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">下載</target>
      </trans-unit>
      <trans-unit id="End time">
        <source>End time</source>
        <note>Title for a end time (on which to visit an attraction)</note>
        <target xml:lang="zh-Hant" state="translated">結束時間</target>
      </trans-unit>
      <trans-unit id="Evening">
        <source>Evening</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">傍晚</target>
      </trans-unit>
      <trans-unit id="Female">
        <source>Female</source>
        <note>Identifying the game is for female division</note>
        <target xml:lang="zh-Hant" state="translated">女子組</target>
      </trans-unit>
      <trans-unit id="Final">
        <source>Final</source>
        <note>Indicating the game is final, with award ceremony afterwards</note>
        <target xml:lang="zh-Hant" state="translated">決賽</target>
      </trans-unit>
      <trans-unit id="Foursquare Tips">
        <source>Foursquare Tips</source>
        <note>Title for cell that shows a list of Foursquare tips. Note, only Tips needs to be translated</note>
        <target xml:lang="zh-Hant" state="translated">Foursquare 小貼士</target>
      </trans-unit>
      <trans-unit id="Games">
        <source>Games</source>
        <note>Search scope (category) name</note>
        <target xml:lang="zh-Hant" state="translated">奧運</target>
        <alt-trans>
          <target xml:lang="zh-Hant">奧運項目</target>
        </alt-trans>
      </trans-unit>
      <trans-unit id="Gender">
        <source>Gender</source>
        <note>Title for cell in detail view that shows the gender, applicable for Oympic game type of events</note>
        <target xml:lang="zh-Hant" state="translated">性別</target>
      </trans-unit>
      <trans-unit id="Hotels">
        <source>Hotels</source>
        <note>Search scope (category) name</note>
        <target xml:lang="zh-Hant" state="translated">住宿</target>
      </trans-unit>
      <trans-unit id="Hours">
        <source>Hours</source>
        <note>Title for cell in detail view that shows the event's opening hours</note>
        <target xml:lang="zh-Hant" state="translated">營業時間</target>
      </trans-unit>
      <trans-unit id="How it works">
        <source>How it works</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">我們如何達成</target>
      </trans-unit>
      <trans-unit id="Locations">
        <source>Locations</source>
        <note>Search scope (category) name</note>
        <target xml:lang="zh-Hant" state="translated">地點</target>
      </trans-unit>
      <trans-unit id="Male">
        <source>Male</source>
        <note>Identifying the game is for male division</note>
        <target xml:lang="zh-Hant" state="translated">男子組</target>
      </trans-unit>
      <trans-unit id="Max Date">
        <source>Max Date</source>
        <note>Max date used to filter results</note>
        <target xml:lang="zh-Hant" state="translated">結束</target>
      </trans-unit>
      <trans-unit id="Min Date">
        <source>Min Date</source>
        <note>Min date used to filter results</note>
        <target xml:lang="zh-Hant" state="translated">起始</target>
      </trans-unit>
      <trans-unit id="Mixed">
        <source>Mixed</source>
        <note>Identifying the game contains both male and female divisions</note>
        <target xml:lang="zh-Hant" state="translated">男女混合</target>
      </trans-unit>
      <trans-unit id="More tips from Foursquare">
        <source>More tips from Foursquare</source>
        <note>Title for button that takes users to view all tips on Foursquare</note>
        <target xml:lang="zh-Hant" state="translated">更多來自Foursquare的小貼士</target>
      </trans-unit>
      <trans-unit id="Morning">
        <source>Morning</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">早晨</target>
        <alt-trans>
          <target xml:lang="zh-Hant">早上</target>
        </alt-trans>
      </trans-unit>
      <trans-unit id="My agenda">
        <source>My agenda</source>
        <note>Title for section in detail view that lists the dates/times to attend the event</note>
        <target xml:lang="zh-Hant" state="translated">我的行程</target>
      </trans-unit>
      <trans-unit id="New to Rio de Janeiro? Don’t worry! Use RioGo to get to your events in time and without hassle.">
        <source>New to Rio de Janeiro? Don’t worry! Use RioGo to get to your events in time and without hassle.</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">剛抵達里約熱內盧？不用擔心．使用RioGo可以讓您輕鬆抵達您所排定的活動</target>
      </trans-unit>
      <trans-unit id="Pick a date">
        <source>Pick a date</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">選擇一個日期</target>
      </trans-unit>
      <trans-unit id="Pick a time">
        <source>Pick a time</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">選擇一個時間</target>
      </trans-unit>
      <trans-unit id="Please add your holiday information and RioGo will take care of the rest">
        <source>Please add your holiday information and RioGo will take care of the rest</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">請添加您的假期信息，我們會處理剩餘的事情</target>
      </trans-unit>
      <trans-unit id="Please pick a type for this location">
        <source>Please pick a type for this location</source>
        <note>Prompted when adding a location of unknown type to the agenda. Possible types would be Stay, Attraction, Restaurant, Place.</note>
        <target xml:lang="zh-Hant" state="translated">請選擇此位置的類型</target>
      </trans-unit>
      <trans-unit id="Powered by ">
        <source>Powered by </source>
        <note>E.g., Powered by tripAdvisor</note>
        <target xml:lang="zh-Hant" state="translated">資料提供商 </target>
      </trans-unit>
      <trans-unit id="Powered by Foursquare">
        <source>Powered by Foursquare</source>
        <note>Note, only Powered by needs to be translated</note>
        <target xml:lang="zh-Hant" state="translated">資料由Foursquare提供</target>
      </trans-unit>
      <trans-unit id="Read all reviews">
        <source>Read all reviews</source>
        <note>Title for button that takes users to view all reviews on TripAdvisor</note>
        <target xml:lang="zh-Hant" state="translated">查看所有的評論</target>
      </trans-unit>
      <trans-unit id="Read full reivew">
        <source>Read full reivew</source>
        <note>Title for button that takes users to full review of the event</note>
        <target xml:lang="zh-Hant" state="translated">查看完整的評論</target>
      </trans-unit>
      <trans-unit id="Read full tip">
        <source>Read full tip</source>
        <note>Title for button that takes users to full tip of the event</note>
        <target xml:lang="zh-Hant" state="translated">查看完整的小貼士</target>
      </trans-unit>
      <trans-unit id="Remove">
        <source>Remove</source>
        <note>Title for button that allows users to remove an event from agenda</note>
        <target xml:lang="zh-Hant" state="translated">移除</target>
      </trans-unit>
      <trans-unit id="Restaurant">
        <source>Restaurant</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">餐館</target>
      </trans-unit>
      <trans-unit id="Restaurants">
        <source>Restaurants</source>
        <note>Search scope (category) name</note>
        <target xml:lang="zh-Hant" state="translated">餐館</target>
      </trans-unit>
      <trans-unit id="Schedules">
        <source>Schedules</source>
        <note>Title for view that shows the list of events under one Games game title, e.g., men's heat 1, heat 2...etc</note>
        <target xml:lang="zh-Hant" state="translated">排定時間表</target>
      </trans-unit>
      <trans-unit id="Select time">
        <source>Select time</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">另選時間</target>
        <alt-trans>
          <target xml:lang="zh-Hant">選擇時間</target>
        </alt-trans>
      </trans-unit>
      <trans-unit id="Set dates">
        <source>Set dates</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">設置日期</target>
      </trans-unit>
      <trans-unit id="Set times">
        <source>Set times</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">設置時間</target>
      </trans-unit>
      <trans-unit id="Set your hotel location">
        <source>Set your hotel location</source>
        <note>Title for button that allows users to pick their hotels</note>
        <target xml:lang="zh-Hant" state="translated">設置您的酒店位置</target>
      </trans-unit>
      <trans-unit id="Set your stay location">
        <source>Set your stay location</source>
        <note>Prompt for user to specify their hotel/stay location</note>
        <target xml:lang="zh-Hant" state="translated">設置您的住宿位置</target>
      </trans-unit>
      <trans-unit id="Show list">
        <source>Show list</source>
        <note>Title for button that switches the agenda to list mode</note>
        <target xml:lang="zh-Hant" state="translated">顯示清單</target>
      </trans-unit>
      <trans-unit id="Simply enter your hotel, events and other attractions you’d like to see. RioGo will create the perfect itinerary for you each day.">
        <source>Simply enter your hotel, events and other attractions you’d like to see. RioGo will create the perfect itinerary for you each day.</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">您只要告訴我們您的酒店，想看的奧運比賽，想去的風景名勝，我們會為您規劃每天最完美的行程！</target>
      </trans-unit>
      <trans-unit id="Start time">
        <source>Start time</source>
        <note>Title for a start time (on which to visit an attraction)</note>
        <target xml:lang="zh-Hant" state="translated">開始時間</target>
      </trans-unit>
      <trans-unit id="Stay">
        <source>Stay</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">住宿</target>
      </trans-unit>
      <trans-unit id="Stay allows you to select check in and check out times while others allow you to pick an attendance date">
        <source>Stay allows you to select check in and check out times while others allow you to pick an attendance date</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">住宿可讓您選擇入住與退房的日期，其他類型可讓您選擇想去的日期．</target>
      </trans-unit>
      <trans-unit id="This event has already been added">
        <source>This event has already been added</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">此項目已在您的行程中</target>
      </trans-unit>
      <trans-unit id="Time">
        <source>Time</source>
        <note>Title for cell in detail view that shows the event's time, applicable for Oympic game type of events</note>
        <target xml:lang="zh-Hant" state="translated">時間</target>
      </trans-unit>
      <trans-unit id="TripAdvisor® Traveler Reviews">
        <source>TripAdvisor® Traveler Reviews</source>
        <note>Title for cell that shows a list of TripAdvisor reviews. Note, only Traveler Reviews needs to be translated</note>
        <target xml:lang="zh-Hant" state="translated">TripAdvisor® 旅行者評論</target>
      </trans-unit>
      <trans-unit id="TripGo is recommended for optimal experience. You can install it from App Store.">
        <source>TripGo is recommended for optimal experience. You can install it from App Store.</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">我們推薦您使用TripGo來達到最佳的使用經驗．您可從App Store取得．</target>
      </trans-unit>
      <trans-unit id="TripGo not found">
        <source>TripGo not found</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">您未安裝TripGo</target>
      </trans-unit>
      <trans-unit id="View agenda">
        <source>View agenda</source>
        <note>No comment provided by engineer.</note>
        <target xml:lang="zh-Hant" state="translated">查看行程</target>
      </trans-unit>
      <trans-unit id="View trips">
        <source>View trips</source>
        <note>Title for button that allows users to either view trips in browser or in TripGo</note>
        <target xml:lang="zh-Hant" state="translated">查看詳細路線</target>
      </trans-unit>
      <trans-unit id="Website">
        <source>Website</source>
        <note>Title for cell in detail view that shows the event's contact website</note>
        <target xml:lang="zh-Hant" state="translated">網站</target>
      </trans-unit>
    </body>
  </file>
